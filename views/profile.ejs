<!DOCTYPE html>
<html lang="en">
<head>
    <title>Profile Pages</title>
</head>
<body>
    <%- include('common/header'); %>
    <h2>Profile Pages</h2>
    <h2>Name<%= user.name%></h2>
    <h2>City<%= user.city%></h2>
    <h2>Email<%= user.email%></h2>
    <ul>
        <% user.skills.forEach((item) =>{ %>
            <li><%= item %></li>
        <% }) %>
    </ul>
</body>
</html>